{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Launch Main",
          "type": "cppdbg",
          "request": "launch",
          "program": "${workspaceFolder}/bin/posix/src/ports/POSIX/OpENer",
          "args": ["eth0"],
          "stopAtEntry": false,
          "cwd": "${workspaceFolder}",
          "environment": [],
          "externalConsole": false,
          "MIMode": "gdb",
          "setupCommands": [
              {
                  "description": "Enable pretty-printing for gdb",
                  "text": "-enable-pretty-printing",
                  "ignoreFailures": true
              }
          ],
          "preLaunchTask": "build OpENer"
      }
  ]
}